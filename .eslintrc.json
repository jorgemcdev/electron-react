{
  "extends": "airbnb",
  "parserOptions":{
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
        "consistent-return": 0,
        "react/prefer-stateless-function": 0,
        "react/jsx-filename-extension": 0,
        "no-shadow": 0,
        "no-console": 0,
        "comma-dangle": 0,
        "no-underscore-dangle": 0,
        "no-unused-vars": 0,
        "max-len": [1, 120, 2, {"ignoreComments": true}],
        "react/prop-types": 0,
        "import/no-extraneous-dependencies": 0
    }
}